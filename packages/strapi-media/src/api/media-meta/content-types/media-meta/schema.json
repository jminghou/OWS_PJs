{
  "kind": "collectionType",
  "collectionName": "media_metas",
  "info": {
    "singularName": "media-meta",
    "pluralName": "media-metas",
    "displayName": "MediaMeta",
    "description": "媒體附加元資料"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "chartid": {
      "type": "string",
      "maxLength": 18,
      "regex": "^[0-9]{18}$"
    },
    "place": {
      "type": "string",
      "maxLength": 255
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "media_metas"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "media_metas"
    },
    "copyright": {
      "type": "string",
      "maxLength": 255
    },
    "isPublic": {
      "type": "boolean",
      "default": false
    }
  }
}
