{
  "name": "strapi-media",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build && npm run copy-config",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "dev": "npm run start:dev",
    "develop": "node scripts/develop.js",
    "develop:raw": "strapi develop",
    "start:dev": "npm run build && strapi start",
    "copy-config": "node -e \"const fs=require('fs'); fs.mkdirSync('dist/config',{recursive:true}); fs.readdirSync('config').forEach(f=>fs.copyFileSync('config/'+f,'dist/config/'+f));\"",
    "start": "strapi start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry"
  },
  "dependencies": {
    "@strapi-community/strapi-provider-upload-google-cloud-storage": "^5.0.5",
    "@strapi/plugin-cloud": "5.34.0",
    "@strapi/plugin-users-permissions": "5.34.0",
    "@strapi/strapi": "5.34.0",
    "better-sqlite3": "^12.6.2",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "styled-components": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=20.0.0 <=24.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "c5c1c79e-161e-49dc-b6e1-b57c98e8d23e",
    "installId": "fd2b2c3d1e3b6efe644ee970eb493daaeba63c0ae36a94f668af9f5f5e55b5e6"
  }
}
