{
  "name": "ows-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/ui",
    "sites/*/frontend"
  ],
  "scripts": {
    "dev:polaris": "npm run dev --workspace=p-ows-frontend",
    "build:polaris": "npm run build --workspace=p-ows-frontend",
    "docker:backend": "docker-compose up -d db redis polaris_backend",
    "docker:stop": "docker-compose down",
    "install:all": "npm install"
  }
}
